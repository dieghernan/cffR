    c(person(given = "First",
             family = "Author",
             role = c("aut", "cre"),
             email = "first.last@example.com",
             comment = c(ORCID = "0000-0001-8457-4658")),
      person(given = "Second",
             family = "Author",
             role = "cre",
             email = "second.last@example.com",
             comment = c(affiliation = "An affiliation", alias = "nickname")),
      person(given = "I am an entity",
             role = "cre",
             comment = c("date-end" = "2020-01-01")),
      person(given = "I should not appear on the CITATION",
             role = "cph",
             comment = "I am not author or creator"))
